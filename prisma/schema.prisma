// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator json {
  provider = "node ./node_modules/prisma-json-schema-generator"
  output   = "."
}

model User {
  id_        Int    @id @default(autoincrement())
  first_name String
  last_name  String
  year_name  Int
  location String @default("")
}

// THIS table is generated by cqx to manage token information.

model Token_ {
  id         Int      @id @default(autoincrement())
  value      String   @unique @db.Text
  pass       String
  createdAt_ DateTime @default(now())
}
